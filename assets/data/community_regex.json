[
  {
    "name": "username",
    "regex": "^[a-z0-9_-]{3,16}$"
  },
  {
    "name": "Password",
    "regex": "^(?=.*?[a-z])(?=.*?[A-Z])(?=.*?[0-9])(?=.*?[!@#$%^&*]).{8,100}$"
  },
  {
    "name": "Common email ids",
    "regex": "([\\w+-.%]+@[\\w-.]+\\.[A-Za-z]{2,4})((,[\\w+-.%]+@[\\w-.]+\\.[A-Za-z]{2,4}){1,})?"
  },
  {
    "name": "IPv4 (1)",
    "regex": "^(([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])$"
  },
  {
    "name": "IPv4 (2)",
    "regex": "\\b((25[0-5]|2[0-4]\\d|[01]?\\d?\\d)\\.){3}(25[0-5]|2[0-4]\\d|[01]?\\d?\\d)\\b"
  },
  {
    "name": "TAG",
    "regex": "</?[\\w\\s]*>|<.+[\\W]>"
  },
  { "name": "Duplicates", "regex": "(\\b\\w+\\b)(?=.*\\b\\1\\b)" },
  { "name": "Hex color", "regex": "^#([a-f0-9A-F]{6}|[0-9a-fA-F]{3})\\b$" },

  {
    "name": "Date (MM/dd/yyyy)",
    "regex": "\\b(?<MM>[01]?[0-9])\\/(?<DD>[0-3]?[0-9])\\/(?<YYYY>[0-2][0-9][0-9][0-9])\\b"
  }
]
